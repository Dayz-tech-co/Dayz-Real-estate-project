											"// Visualization Script",
											"// Visualization Script",
											"const template = `",
											"<canvas id=\"myChart\" height=\"75\"></canvas>",
											"<script src=\"https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js\"></script>",
											"<script>",
											"    var ctx = document.getElementById(\"myChart\");",
											"    var myChart = new Chart(ctx, {",
											"        type: \"bar\",",
											"        data: {",
											"            labels: [], // Will be filled by pm.getData",
											"            datasets: [{",
											"                label: 'Values',",
											"                data: [],",
											"                backgroundColor: 'rgba(54, 162, 235, 0.5)',",
											"                borderColor: 'rgba(54, 162, 235, 1)',",
											"                borderWidth: 1",
											"            }]",
											"        },",
											"        options: {",
											"            legend: { display: true },",
											"            title: {",
											"                display: true,",
											"                text: 'Response Data Chart'",
											"            },",
											"            scales: {",
											"                xAxes: [{",
											"                    display: true,",
											"                    scaleLabel: {",
											"                        display: true,",
											"                        labelString: 'Keys'",
											"                    }",
											"                }],",
											"                yAxes: [{",
											"                    display: true,",
											"                    scaleLabel: {",
											"                        display: true,",
											"                        labelString: 'Values'",
											"                    },",
											"                    ticks: {",
											"                        beginAtZero: true",
											"                    }",
											"                }]",
											"            }",
											"        }",
											"    });",
											"",
											"    pm.getData(function (err, value) {",
											"        myChart.data.labels = value.labels;",
											"        myChart.data.datasets[0].data = value.data;",
											"        myChart.update();",
											"    });",
											"</script>`;",
											"",
											"function constructVisualizerPayload() {",
											"    let json = {};",
											"    try {",
											"        json = pm.response.json();",
											"    } catch (e) {",
											"        return { labels: [], data: [] };",
											"    }",
											"    // If the response is an array of objects with a common numeric property, chart that property",
											"    if (Array.isArray(json) && json.length > 0 && typeof json[0] === 'object') {",
											"        // Find the first numeric property",
											"        const keys = Object.keys(json[0]);",
											"        let valueKey = keys.find(k => typeof json[0][k] === 'number');",
											"        if (!valueKey) return { labels: [], data: [] };",
											"        return {",
											"            labels: json.map((item, i) => item.name || item.id || `Item ${i+1}`),",
											"            data: json.map(item => item[valueKey])",
											"        };",
											"    }",
											"    // If the response is an object with numeric values, chart those",
											"    if (typeof json === 'object' && !Array.isArray(json)) {",
											"        const keys = Object.keys(json).filter(k => typeof json[k] === 'number');",
											"        return {",
											"            labels: keys,",
											"            data: keys.map(k => json[k])",
											"        };",
											"    }",
											"    return { labels: [], data: [] };",
											"}",
											"",
											"pm.visualizer.set(template, constructVisualizerPayload());",
											""
